import java.util.*;

public class Conditional {
    public static void main(String[] args){
        Scanner st = new Scanner(System.in);
      int A=10, B = 15, C = 6;
      if( A>=B && A>=C ){
          System.out.println("A is greater");
      }else if(B>=C){
          System.out.println("B is Greater");
      }
      else{
          System.out.println("C is Greater");
      }
    }
}
